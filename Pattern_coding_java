
import java.util.Scanner;
public class Main
{
    Scanner sc=new Scanner(System.in);
	public static void main(String[] args) {
	for(int row=1;row<=5;row++){
int n1=1;
char ch ='A';
for(int col = 1; col <=row ; col++){
if(row%2==1){
System.out.print(n1+" ");
n1++;
}
else{
System.out.print(ch+" ");
ch++;
}
}
System.out.println();
}
	}
}
